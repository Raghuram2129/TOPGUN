#include<bits/stdc++.h>
using namespace std;
int main()
{
	int arr[]={10,20,30,40};
	int n=4;
	int rev[n];
	for(int i=0;i<n;i++)
	{
		rev[i]=arr[n-1-i];
	}
	for(int i:rev)
	{
		cout<<i<<" ";
	}
	vector<int >arr={10,20,30,40,50};
	reverse(arr.begin(),arr.end());
	for(int i:arr)
	{
		cout<<i<<" ";
	}
}
